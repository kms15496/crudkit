{
  "name": "kms15496/crudkit",
  "description": "Drop-in generic CRUD for Laravel",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Kaung\\CrudKit\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Kaung\\CrudKit\\CrudKitServiceProvider",
        "Mcamara\\LaravelLocalization\\LaravelLocalizationServiceProvider"
      ]
    }
  },
  "require": {
    "php": "^8.2",
    "laravel/framework": "^10.0|^11.0|^12.0",
    "spatie/laravel-medialibrary": "^11.0",
    "mcamara/laravel-localization": "^2.2"
  }
}
